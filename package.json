{
	"name": "cosmwasm",
	"displayName": "VsCode CosmWasm",
	"description": "CosmWasm build & optimize and simulate",
	"icon": "logo-128.png",
	"version": "0.13.1",
	"publisher": "oraichain",
	"repository": {
		"type": "git",
		"url": "https://github.com/oraichain/vscode-cosmwasm.git"
	},
	"engines": {
		"vscode": "^1.53.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/src/extension",
	"contributes": {
		"configuration": {
			"title": "CosmWasm Configuration",
			"properties": {
				"cosmwasm.tool.build": {
					"type": "string",
					"default": "cosmwasm-optimize",
					"description": "Build & optimize cosmwasm contract into wasm file"
				},
				"cosmwasm.tool.simulate": {
					"type": "string",
					"default": "cosmwasm-simulate",
					"description": "Simulate the wasm file"
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint . --ext .ts,.tsx",
		"watch": "tsc -watch -p ./",
		"test": "node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"@types/mocha": "^9.0.0",
		"@types/node": "^12.12.0",
		"mocha": "^9.1.0",
		"typescript": "^4.3.5",
		"@types/vscode": "^1.53.0"
	}
}